{
  "name": "boilerplate-testing-framework",
  "version": "0.1.0",
  "description": "Boilerplate including Babel and Webpack for building/transpiling, Jasmine and Karma for testing and Istanbul for code coverage.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --env development",
    "build:prod": "webpack -p --env production",
    "test": "karma start",
    "coverage": "karma start --single-run --no-auto-watch --coverage=true || echo done"
  },
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "webpack 2",
    "jasmine",
    "karma",
    "istanbul",
    "boilerplate"
  ],
  "author": "Richard Franklin",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "webpack": "^4.14.0",
    "webpack-dev-server": "^3.1.4"
  }
}
